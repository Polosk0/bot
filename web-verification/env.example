# Configuration Discord OAuth2
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
DISCORD_REDIRECT_URI=https://emynona.shop/auth/callback

# URL du site web (pour les liens dans les embeds Discord)
WEB_VERIFICATION_URL=https://emynona.shop

# Configuration Serveur
GUILD_ID=your_discord_guild_id
PORT=3000

# Configuration Bot API (interne, reste en localhost)
BOT_API_URL=http://localhost:3001
BOT_API_KEY=your_secure_api_key_here

# Configuration Discord Interactions (optionnel, pour Discord Activities)
# DISCORD_PUBLIC_KEY=your_discord_public_key_here

# Configuration Production
NODE_ENV=development






