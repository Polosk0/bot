# Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here

# Channel IDs
LOG_CHANNEL_ID=your_log_channel_id
TICKET_CHANNEL_ID=your_ticket_channel_id
VOUCH_CHANNEL_ID=your_vouch_channel_id
VERIFICATION_CHANNEL_ID=your_verification_channel_id
DEBUG_CHANNEL_ID=your_debug_channel_id

# Role IDs
ADMIN_ROLE_ID=your_admin_role_id
MODERATOR_ROLE_ID=your_moderator_role_id
VOUCH_ROLE_ID=your_vouch_role_id
VERIFIED_ROLE_ID=your_verified_role_id

# Database (optionnel)
DATABASE_URL=your_database_url_here

# Bot HTTP Server (pour l'API de vérification)
BOT_HTTP_PORT=3001
BOT_API_KEY=your_secure_api_key_here

# URL du site web de vérification (pour les liens dans les embeds)
WEB_VERIFICATION_URL=http://93.127.160.64:3000

# Système de vérification par webhook (comme Restorecord)
# Mettez à 'true' pour activer les webhooks Discord pour la vérification manuelle
USE_WEBHOOK_VERIFICATION=false

